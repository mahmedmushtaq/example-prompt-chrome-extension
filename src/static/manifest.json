{
  "manifest_version": 3,
  "name": "ChatGpt Example Prompts",
  "version": "1.0.0",
  "description": "This extension will help you to generate command without writing it explicitly",
  "icons": {
    "16": "icon.png"
  },
  "action": {
    "default_title": "click me",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "bookmarks"],
  "content_scripts": [
    { "matches": ["https://*.google.com/*"], "js": ["contentScript.js"] }
  ]
}
